{
  "name": "unicafe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "start:ssr": "cross-env SSR=true node index.js",
    "start:csr": "cross-env SSR=false node index.js",
    "dev:f": "vite Frontend",
    "build": "pnpm build:c && pnpm build:s",
    "build:c": "vite build Frontend --outDir ../dist/client",
    "build:s": "vite build Frontend --ssr src/entry-server.jsx --outDir ../dist/server",
    "lint": "pnpm lint:f && pnpm lint:b",
    "lint:fix": "pnpm lint:f --fix && pnpm lint:b --fix",
    "lint:f": "eslint -c ./Frontend/eslint.config.js Frontend",
    "lint:b": "eslint -c ./eslint.config.js ."
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^5.6.1",
    "bootstrap": "^5.3.8",
    "compression": "^1.8.1",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "react": "^19.2.3",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.3",
    "sirv": "^3.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "nodemon": "^3.1.11",
    "vite": "^7.3.0"
  }
}
